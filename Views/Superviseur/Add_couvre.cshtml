


@{
    ViewData["Title"] = "Display Pack Page";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Add Couvre</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">Ã—</span>
                </button>
            </div>
            <div class="modal-body">
                <form method="post" enctype="multipart/form-data" id="addCouvreForm" asp-action="Add_couvre">
                    <div class="form-group">
                        <label for="exampleFormControlInput1" >Image_couvre</label>
                        <input type="file" class="form-control" name="formFile"  id="exampleFormControlInput1" placeholder="Saisir la superficie">
                    </div>
                    <div class="form-group">
                        <label for="exampleFormControlInput1" >Nom</label>
                        <input type="text" class="form-control" name="Nom" id="exampleFormControlInput1" placeholder="saisir la reduction">
                    </div>
                      <div class="form-group">
                        <label for="exampleFormControlInput1" >Main_oeuvre</label>
                        <input type="number" class="form-control" name="main_oeuvre"  id="exampleFormControlInput1" placeholder="saisir la reduction">
                    </div>
                        <div class="form-group">
                        <label for="exampleFormControlInput1" >Materiaux</label>
                        <input type="number" class="form-control" name="materieaux" id="exampleFormControlInput1" placeholder="saisir la reduction">
                    </div>
                    <div class="modal-footer ">
                        <button type="submit" class="btn btn-primary" >Save changes</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
